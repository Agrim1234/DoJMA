<resources>

    <!-- Declare custom theme attributes that allow changing which styles are
         used for button bars depending on the API level.
         ?android:attr/buttonBarStyle is new as of API 11 so this is
         necessary to support previous API levels. -->
    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>


    <attr name="customNavDrawerTitleTextColor" format="color" />
    <attr name="customNavDrawerIconTintColor" format="color" />


    <attr name="customHeraldTitleTextColor" format="color" />
    <attr name="customHeraldDescTextColor" format="color" />
    <attr name="customHeraldDateTextColor" format="color" />

    <attr name="customGazetteTitleTextColor" format="color" />

    <attr name="customEventsTitleTextColor" format="color" />
    <attr name="customEventsLocationTextColor" format="color" />
    <attr name="customEventsDescTextColor" format="color" />
    <attr name="customEventsDateTextColor" format="color" />
    <attr name="customEventsTimeTextColor" format="color" />
    <attr name="customEventsViewColor" format="color" />

    <attr name="customSearchableErrorTextColor" format="color" />

    <attr name="customLinksTitleTextColor" format="color" />
    <attr name="customLinksLinkTextColor" format="color" />

    <attr name="customContactsNameTextColor" format="color" />

    <attr name="customFavouritesTextColor" format="color" />

    <attr name="defaultBackground" format="color" />

    <attr name="fabBackgroundColor" format="color"/>


</resources>
